{
  "name": "TravelNetwork",
  "version": "0.0.1",
  "description": "Share your travel experiences with your friends",
  "repository": {
    "type": "git",
    "url": "https://github.com/travelNetwork/hybridapp.git"
  },
  "scripts": {
    "postinstall": "remotedev-debugger",
    "start": "node_modules/react-native/packager/packager.sh",
    "eslint": "eslint",
    "test": "jest",
    "android": "ENVFILE=.env.development react-native run-android -- --reset-cache",
    "android:prod": "react-native run-android --variant=release",
    "android:build": "cd android && ./gradlew assembleRelease",
    "ios": "react-native run-ios -- --reset-cache",
    "ios:prod": "react-native run-ios -scheme universal_production",
    "ios:build": "npm run ios:prod",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "reset": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm i",
    "changename": "react-native-rename"
  },
  "dependencies": {
    "color": "^0.11.3",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "native-base": "2.1.4",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.1",
    "react-native-code-push": "2.0.1-beta",
    "react-native-easy-grid": "0.1.8",
    "react-native-modalbox": "^1.3.4",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0",
    "remote-redux-devtools": "^0.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-jest": "17.0.0",
    "babel-preset-react-native": "1.9.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-react-native": "^2.0.0",
    "jest": "17.0.0",
    "jest-react-native": "17.0.0",
    "mocha": "^2.5.3",
    "react-native-rename": "^2.1.5",
    "react-test-renderer": "15.3.2",
    "remote-redux-devtools-on-debugger": "^0.7.0",
    "remotedev-server": "^0.1.2"
  },
  "jest": {
    "preset": "jest-react-native"
  }
}

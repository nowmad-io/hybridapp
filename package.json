{
  "name": "Nowmad",
  "version": "0.0.6",
  "description": "Share your travel experiences with your friends",
  "repository": {
    "type": "git",
    "url": "https://github.com/travelNetwork/hybridapp.git"
  },
  "scripts": {
    "postinstall": "react-native link && npm run workarounds",
    "start": "node_modules/react-native/packager/packager.sh",
    "eslint": "eslint js",
    "avd": "~/Library/Android/sdk/tools/emulator -avd nowmad_avd &",
    "bumpVersion": "node scripts/release.js",
    "workarounds": "node scripts/workarounds.js",
    "android": "ENVFILE=.env.development react-native run-android -- --reset-cache",
    "android:prod": "ENVFILE=.env.release react-native run-android  -- --reset-cache",
    "android:release": "cd android && ENVFILE=.env.release ./gradlew assembleRelease",
    "ios": "react-native run-ios -- --reset-cache",
    "ios:prod": "react-native run-ios -scheme universal_production",
    "ios:release": "npm run ios:prod",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "reset": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && rm -f package-lock.json && npm i",
    "changename": "react-native-rename",
    "generateIcon": "yo rn-toolbox:assets --icon icon.png"
  },
  "dependencies": {
    "color": "3.0.0",
    "lodash": "4.17.5",
    "moment": "2.20.1",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-native": "0.53.0",
    "react-native-code-push": "5.2.1",
    "react-native-config": "0.11.5",
    "react-native-image-picker": "0.26.7",
    "react-native-keyboard-aware-scroll-view": "0.4.3",
    "react-native-maps": "0.20.0",
    "react-native-modalbox": "1.4.2",
    "react-native-snap-carousel": "3.6.0",
    "react-native-vector-icons": "4.5.0",
    "react-navigation": "^1.0.0-beta.27",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-persist": "4.10.1",
    "redux-saga": "0.16.0",
    "shortid": "2.2.8"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-preset-react-native": "4.0.0",
    "eslint": "4.17.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "eslint-plugin-react-native": "3.2.1",
    "generator-rn-toolbox": "3.2.1",
    "react-native-rename": "2.2.2",
    "remote-redux-devtools": "0.5.12",
    "remotedev-server": "0.2.4",
    "rimraf": "^2.6.2"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}

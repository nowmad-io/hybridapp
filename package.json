{
  "name": "TravelNetwork",
  "version": "0.0.2",
  "description": "Share your travel experiences with your friends",
  "repository": {
    "type": "git",
    "url": "https://github.com/travelNetwork/hybridapp.git"
  },
  "scripts": {
    "postinstall": "node node_modules/native-base/ejectTheme.js && react-native link",
    "start": "node_modules/react-native/packager/packager.sh",
    "eslint": "eslint js",
    "test": "jest --no-cache",
    "avd": "~/Library/Android/sdk/tools/emulator -avd nowmad_avd &",
    "android": "ENVFILE=.env.development react-native run-android -- --reset-cache",
    "android:prod": "ENVFILE=.env.release react-native run-android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "ios": "react-native run-ios -- --reset-cache",
    "ios:prod": "react-native run-ios -scheme universal_production",
    "ios:build": "npm run ios:prod",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "reset": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && rm -f package-lock.json && npm i",
    "changename": "react-native-rename"
  },
  "dependencies": {
    "color": "2.0.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "native-base": "2.3.2",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.48.3",
    "react-native-code-push": "5.1.0-beta",
    "react-native-config": "^0.6.1",
    "react-native-easy-grid": "0.1.15",
    "react-native-maps": "^0.16.4",
    "react-native-modalbox": "1.4.2",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-crud-store": "^5.4.0",
    "redux-persist": "4.9.1",
    "redux-saga": "^0.15.6",
    "remote-redux-devtools": "0.5.12"
  },
  "devDependencies": {
    "babel-eslint": "8.0.0",
    "babel-preset-react-native": "4.0.0",
    "chai": "4.1.2",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-react-native": "3.1.0",
    "mocha": "3.5.3",
    "react-native-rename": "2.1.5",
    "remotedev-server": "0.2.4"
  }
}
